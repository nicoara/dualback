@model List<dualback.Domain.Entities.Score>

<!DOCTYPE HTML>
<html>
<head>
    <script type="text/javascript" src="/Scripts/canvasjs.min.js"></script>
    <script type="text/javascript">
        window.onload = function() {
            var chart = new CanvasJS.Chart("chartContainer",
            {
                title: {
                    text: "Score history"
                },
                data: [
                    {
                        type: "line",

                        dataPoints: [
                            @{
                                for (int i = 0; i< Model.Count; i++)
                                {
                                    var score = Model[i].s_Level*100 + Model[i].s_Percent;
                                    string output = "{x: "+i.ToString() + ", y:" + score.ToString() + "},";
                                    @output
                                }
                            }

                        ]
                    }
                ]
            });

            chart.render();
        }
    </script>
</head>
<body>
    <div id="chartContainer" style="height: 500px; width: 50%;"></div>
</body>
</html>
